@model System.Collections.Generic.List<Techshop.Models.Entities.Role>

@{
    ViewBag.Title = "Backoffice";
    Layout = "_BackofficeLayout";
}

<h2>Role List</h2>

<div class="row">
    <table class="table">
        <thead>
        <tr>
            <td class="col-2">Role No.</td>
            <td class="col-2">Name</td>
        </tr>
        </thead>
        <tbody>

        @foreach (var i in Model)
        {
            <tr>
                <td>@i.Id</td>
                <td>@i.Name</td>
            </tr>
        }
        </tbody>
    </table>
</div>